Table:
CREATE TABLE test.CN_AUDIT_SYSTEMS
(
   ID int NOT NULL,
   SYSTEM_ID varchar(10) PRIMARY KEY NOT NULL,
   DESCRIPTION varchar(255),
   CREATED_DT datetime DEFAULT NOW() NOT NULL,
   MODIFIED_DT datetime DEFAULT NOW() NOT NULL
)
Trigger:
CREATE UNIQUE INDEX PK__CN_AUDIT__9C27C76C3CDBDFC0 ON test.CN_AUDIT_SYSTEMS(SYSTEM_ID)