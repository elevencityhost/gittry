CREATE TABLE test.CN_AUDIT_STATUS
(
   ID int NOT NULL,
   STATUS_ID varchar(10) PRIMARY KEY NOT NULL,
   DESCRIPTION varchar(255),
   CREATED_DT datetime DEFAULT NOW() NOT NULL,
   MODIFIED_DT datetime DEFAULT NOW() NOT NULL
)
GO
CREATE UNIQUE INDEX PK__CN_AUDIT__D8827E717AF3EDAE ON test.CN_AUDIT_STATUS(STATUS_ID)
GO