CREATE TABLE test.CN_AUDIT_PHASES
(
   ID int NOT NULL,
   PHASE_ID varchar(10) PRIMARY KEY NOT NULL,
   NAME varchar(255),
   DESCRIPTION varchar(255),
   CREATED_DT datetime DEFAULT NOW() NOT NULL,
   MODIFIED_DT datetime DEFAULT NOW() NOT NULL
)
GO
CREATE UNIQUE INDEX PK__ES_AUDIT__B53D6706CCDF5020 ON test.CN_AUDIT_PHASES(PHASE_ID)
GO


Triggers:
CREATE TRIGGER CN_AUDIT_PHASES_LAST_UPDATE_TRIGGER
AFTER UPDATE ON test.CN_AUDIT_PHASES
FOR EACH ROW SET @MODIFIED_DT=NOW()


